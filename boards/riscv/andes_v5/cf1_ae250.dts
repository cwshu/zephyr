/dts-v1/;

#include <andes_v5_ae250.dtsi>

/ {
	model = "andestech,cf1_ae250";
	compatible = "andestech,cf1_ae250", "andestech,ae250";

	aliases {
		gpio-0 = &gpio0;
		counter-0 = &pit0;
		spi-0 = &spi0;
		spi-1 = &spi1;
		spi-2 = &spi2;
		spi-3 = &spi3;
		i2c-0 = &i2c0;
	};

	chosen {
		zephyr,console = &uart1;
		zephyr,shell-uart = &uart1;
		zephyr,flash = &flash0;
	};

	soc {
		spi2: spi@f1900000 {
			compatible = "andestech,atcspi200";
			label = "spi_2";
			reg = <0xf1900000 0x1000>;
			reg-names = "control";
			#address-cells = <1>;
			#size-cells = <0>;
			num-cs = <1>;
			clock-frequency = <66000000>;
			interrupts = <26 1>;
			interrupt-parent = <&plic0>;
			status = "disabled";
		};

		spi3: spi@f1a00000 {
			compatible = "andestech,atcspi200";
			label = "spi_3";
			reg = <0xf1a00000 0x1000>;
			reg-names = "control";
			#address-cells = <1>;
			#size-cells = <0>;
			num-cs = <1>;
			clock-frequency = <66000000>;
			interrupts = <27 1>;
			interrupt-parent = <&plic0>;
			status = "disabled";
		};
	};
};

&ilm {
	reg = <0x00000000 0x00040000>;
};

&dlm {
	reg = <0x00200000 0x00020000>;
};

&uart1 {
	status = "okay";
	clock-frequency = <19660800>;
	current-speed = <115200>;
};

&gpio0 {
	status = "okay";
};

&pit0 {
	status = "okay";
};

&spi0 {
	status = "okay";
	flash0: flash@0 {
		compatible = "jedec,spi-nor";
		size = <16777216>;
		label = "JEDEC SPI-NOR";
		spi-max-frequency = <1000000>;
		reg = <0>;
		status = "okay";
		jedec-id = [c2 20 16];
	};
};

&spi1 {
	status = "okay";
};

&spi2 {
	status = "okay";
};

&spi3 {
	status = "okay";
};

&i2c0 {
	status = "okay";
};
